{
  "name": "dicty-stock-center",
  "version": "0.0.0-semantically-released",
  "description": "The Dicty Stock Center app rebuilt with React and Redux!",
  "main": "index.js",
  "engines": {
    "node": "14"
  },
  "homepage": "/stockcenter",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom-fourteen",
    "test-coverage": "CI=true react-scripts test --env=jsdom-fourteen --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx",
    "tsc": "tsc",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "semantic-release": "semantic-release",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "cypress:open": "cypress open",
    "cypress:run": "percy exec -- cypress run",
    "cypress:ci": "start-server-and-test start http://localhost:3000 cypress:run",
    "size": "yarn build && size-limit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dictyBase/Dicty-Stock-Center.git"
  },
  "license": "BSD-2-Clause",
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.13",
    "@material-ui/core": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "bowser": "^2.11.0",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#10.2.1",
    "dicty-components-login": "github:dictyBase/dicty-components-login#3.0.1",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#2.0.0",
    "dicty-components-page-editor": "github:dictyBase/dicty-components-page-editor#1.3.1",
    "dicty-hooks": "github:dictyBase/dicty-hooks#1.1.1",
    "element-remove": "^1.0.4",
    "fontsource-roboto": "^3.0.3",
    "formik": "^2.2.2",
    "graphql": "^15.4.0",
    "history": "^5.0.0",
    "immutable": "^3.8.2",
    "jwt-decode": "^3.1.2",
    "querystring": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^3.1.2",
    "react-helmet": "^6.0.0",
    "react-loading-skeleton": "^2.0.1",
    "react-responsive-carousel": "^3.2.11",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.4",
    "react-virtualized-auto-sizer": "^1.0.2",
    "react-window": "^1.8.6",
    "react-window-infinite-loader": "^1.0.5",
    "string.prototype.includes": "^2.0.0",
    "typescript": "^4.1.3",
    "yup": "^0.32.1"
  },
  "devDependencies": {
    "@percy/cypress": "^2.3.1",
    "@semantic-release/git": "^9.0.0",
    "@size-limit/preset-app": "^4.9.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/user-event": "^12.5.0",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.19",
    "@types/jwt-decode": "^3.1.0",
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "@types/react-window": "^1.8.2",
    "@types/react-window-infinite-loader": "^1.0.3",
    "@types/sinon": "^9.0.9",
    "@types/testing-library__react-hooks": "^3.4.1",
    "@types/yup": "^0.29.10",
    "apollo-boost": "^0.4.8",
    "core-js": "^3.8.0",
    "cypress": "^5.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-jest-dom": "^3.6.1",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "4.3.5",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.0",
    "react-docgen-typescript": "^1.20.5",
    "react-styleguidist": "^11.1.5",
    "react-test-renderer": "^16.13.1",
    "semantic-release": "^17.2.4",
    "sinon": "^9.2.2",
    "size-limit": "^4.9.1",
    "source-map-explorer": "^2.5.1",
    "start-server-and-test": "^1.11.6",
    "waait": "^1.0.5",
    "wait-for-expect": "^3.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/app/layout/**/*.{js,jsx,ts,tsx}",
      "src/common/components/**/*.{js,jsx,ts,tsx}",
      "src/common/hooks/**/*.{js,jsx,ts,tsx}",
      "src/common/utils/**/*.{js,jsx,ts,tsx}",
      "src/features/**/*.{js,jsx,ts,tsx}",
      "!src/**/mock*.{js,jsx,ts,tsx}",
      "!src/**/types.{js,jsx,ts,tsx}",
      "!src/**/*Styles.{js,jsx,ts,tsx}",
      "!src/**/clientConfig*.{js,jsx,ts,tsx}"
    ],
    "resetMocks": false
  },
  "lint-staged": {
    "*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "size-limit": [
    {
      "path": "build/static/js/*.js",
      "limit": "800 ms"
    }
  ]
}
